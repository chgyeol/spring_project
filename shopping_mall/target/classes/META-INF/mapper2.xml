<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Shopbag_admin">
<select id="a_count" resultType="int">
select count(*) as ctn from shopbag_admin where aid=#{aid}
</select>
<select id="a_pw" resultType="String">
select apass from shopbag_admin where aid=#{aid}
</select>
<select id="a_all" resultType="admindao">
select * from shopbag_admin where aid=#{aid}
</select>
</mapper>